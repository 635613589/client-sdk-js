{
  "name": "livekit-client",
  "version": "0.1.3",
  "description": "JavaScript/TypeScript client SDK for LiveKit",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:livekit/client-sdk-js.git",
  "author": "David Zhao <david@davidzhao.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn exec tsc",
    "proto": "protoc --plugin=node_modules/ts-proto/protoc-gen-ts_proto --ts_proto_out=./src/proto --ts_proto_opt=outputClientImpl=false,useOptionals=true -I../livekit-server/proto ../livekit-server/proto/rtc.proto ../livekit-server/proto/model.proto",
    "sample": "cd test && webpack serve",
    "test": "jest"
  },
  "dependencies": {
    "@types/ws": "^7.4.0",
    "events": "^3.2.0",
    "loglevel": "^1.7.1",
    "protobufjs": "^6.10.2",
    "webrtc-adapter": "^7.7.0"
  },
  "devDependencies": {
    "@webpack-cli/serve": "^1.2.1",
    "ts-loader": "^8.0.11",
    "ts-proto": "1.41.0",
    "typescript": "^4.1.2",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  }
}
