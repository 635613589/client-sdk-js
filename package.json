{
  "devDependencies": {
    "ts-proto": "^1.39.0",
    "typescript": "^4.1.2"
  },
  "name": "livekit-js",
  "version": "0.0.1",
  "description": "JavaScript/TypeScript SDK for LiveKit",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:livekit/livekit-js.git",
  "author": "David Zhao <david@davidzhao.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn exec tsc",
