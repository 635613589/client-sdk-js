{
  "name": "livekit-client",
  "version": "0.5.5",
  "description": "JavaScript/TypeScript client SDK for LiveKit",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:livekit/client-sdk-js.git",
  "author": "David Zhao <david@davidzhao.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn exec tsc",
    "build-docs": "yarn exec typedoc",
    "proto": "protoc --plugin=node_modules/ts-proto/protoc-gen-ts_proto --ts_proto_out=./src/proto --ts_proto_opt=outputClientImpl=false,useOptionals=true -I../protocol ../protocol/rtc.proto ../protocol/model.proto",
    "sample": "cd example && webpack serve",
    "test": "jest"
  },
  "dependencies": {
    "@types/semver": "^7.3.4",
    "@types/ws": "^7.4.0",
    "events": "^3.2.0",
    "loglevel": "^1.7.1",
    "protobufjs": "^6.10.2",
    "semver": "^6.3.0",
    "webrtc-adapter": "^7.7.0"
  },
  "devDependencies": {
    "@webpack-cli/serve": "^1.2.1",
    "ts-loader": "^8.0.11",
    "ts-proto": "1.41.0",
    "typedoc": "^0.20.25",
    "typedoc-plugin-no-inherit": "^1.2.0",
    "typescript": "^4.1.2",
    "webpack": "^5.9.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  }
}
